{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "prettier"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "quotes": [2, "single", "avoid-escape"],
    "no-console": "off",
    "indent": ["error", 4],
    "prettier/prettier": "error"
  }
}
